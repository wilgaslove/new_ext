{
  "name": "vscode-server-connector-api",
  "version": "0.1.7",
  "description": "",
  "main": "lib/src/main.js",
  "types": "lib/src/main.d.ts",
  "files": [
    "lib/src/**/*.js",
    "lib/src/**/*.d.ts"
  ],
  "scripts": {
    "prepare": "node ./node_modules/vscode/bin/install && npm run compile",
    "build": "rimraf lib && tslint -p tslint.json && tsc",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "echo \"No tests have been provided yet\"",
    "coverage:upload": "codecov -f coverage/*.json"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "lib",
      "test",
      "coverage"
    ],
    "reporter": [
      "json",
      "html",
      "text",
      "lcov",
      "text-summary"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redhat-developer/vscode-server-connector-api.git"
  },
  "keywords": [
    "runtime",
    "server",
    "adapters",
    "protocol"
  ],
  "author": "Red Hat Developers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redhat-developer/vscode-server-connector-api/issues"
  },
  "homepage": "https://github.com/redhat-developer/vscode-server-connector-api/issues",
  "engines": {
    "vscode": "^1.31.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.26",
    "@types/sinon": "^5.0.1",
    "@types/sinon-chai": "^3.2.0",
    "chai": "^4.1.2",
    "codecov": "^3.2.0",
    "gulp": "^4.0.0",
    "gulp-tslint": "^8.1.2",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3",
    "sinon": "^6.1.5",
    "sinon-chai": "^3.2.0",
    "source-map-support": "^0.5.8",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.3",
    "vscode": "^1.1.34"
  }
}
